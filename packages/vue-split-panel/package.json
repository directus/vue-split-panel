{
	"name": "@directus/vue-split-panel",
	"type": "module",
	"version": "0.8.4",
	"description": "Split panel component for Vue based on WebAwesome Split Panel",
	"author": "Rijk van Zanten <rijkvanzanten@me.com>",
	"homepage": "https://github.com/directus/vue-split-panel",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/directus/vue-split-panel.git"
	},
	"bugs": {
		"url": "https://github.com/directus/vue-split-panel/issues"
	},
	"keywords": [
		"vue",
		"ui",
		"components",
		"panel",
		"splitter"
	],
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json",
		"./index.css": "./dist/index.css"
	},
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"playground": "vite",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"typecheck": "vue-tsc --noEmit",
		"release": "bumpp && pnpm publish",
		"prepublishOnly": "pnpm run build",
		"lint": "eslint --flag v10_config_lookup_from_file --cache .",
		"lint:fix": "pnpm run lint --fix"
	},
	"peerDependencies": {
		"vue": "^3.5.0"
	},
	"dependencies": {
		"@vueuse/core": "13.9.0",
		"test": "3.3.0"
	},
	"devDependencies": {
		"@types/node": "24.7.2",
		"@vitejs/plugin-vue": "6.0.1",
		"@vitest/coverage-v8": "3.2.4",
		"@vue/test-utils": "2.4.6",
		"bumpp": "10.3.1",
		"eslint": "9.37.0",
		"happy-dom": "20.0.2",
		"tsdown": "0.15.7",
		"typescript": "5.9.3",
		"vite": "npm:rolldown-vite@latest",
		"vitest": "3.2.4",
		"vue": "3.5.19",
		"vue-tsc": "3.1.1"
	}
}
